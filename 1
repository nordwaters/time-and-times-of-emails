name = input("Enter file:")
if len(name) < 1 : name = "mbox-short.txt"
handle = open(name)
counts = dict()

for line in handle:
    line = line.rstrip()
    words = line.split()
    if len(words) < 5:
        continue
    if words[0] != 'From':
        continue
    words = list(words[5].split(':'))
    hours = list(words[0].split())
    for hour in hours:
        counts[hour] = counts.get(hour, 0) + 1
lst = list()
for key, val in counts.items():
    newtup = (val, key)
    lst.append(newtup)

lst = sorted(lst)

for key in sorted(counts.keys()):
    print(key, counts[key])
